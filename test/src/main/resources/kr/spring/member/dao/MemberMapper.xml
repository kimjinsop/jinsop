<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper   
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.spring.member.dao.MemberMapper">
	<select id="list" parameterType="map" resultType="memberCommand">
		SELECT * FROM (SELECT a.*, rownum rnum FROM
		(SELECT * FROM member
		<!-- <where> <if test="keyword !='' and keyfield=='all'"> mem_id like '%' 
			|| #{keyword} || '%' or mem_name like '%' || #{keyword} || '%' or mem_cell 
			like '%' || #{keyword} || '%' </if> </where> -->
		ORDER BY mem_id DESC)a where mem_id!='삭제') 
	<![CDATA[
	WHERE rnum >= #{start} AND rnum <= #{end}
	]]>
	</select>

</mapper>